                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="50"
                    android:layout_marginRight="10dp"
                    android:text="@string/district" />

                <TextView
                    android:id="@+id/txtDistrict"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="50"
                    android:gravity="right"
                    android:text="" />
            </LinearLayout>
            
        </LinearLayout>
        
        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="20dp" >

            <LinearLayout
                android:id="@+id/LinearLayout06"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="20dp"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/imgHouse1"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="@drawable/bgimagecapture"
                    android:src="@drawable/capture" />

                <TextView
                    android:id="@+id/TextView05"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="@string/photo_house1" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/LinearLayout05"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/imgHouse2"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="@drawable/bgimagecapture"
                    android:src="@drawable/capture" />

                <TextView
                    android:id="@+id/TextView04"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="@string/photo_house2" />
            </LinearLayout>

            
        </LinearLayout>

        <LinearLayout
            android:id="@+id/LinearLayout01"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:layout_marginBottom="40dp" >

            <LinearLayout
                android:id="@+id/LinearLayout07"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="20dp"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/imgHouse3"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="@drawable/bgimagecapture"
                    android:src="@drawable/capture" />

                <TextView
                    android:id="@+id/TextView06"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="@string/photo_house3" />
            </LinearLayout>
            
            <LinearLayout
                android:id="@+id/LinearLayout10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/imgConnection"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="@drawable/bgimagecapture"
                    android:src="@drawable/capture" />

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="@string/photo_premise_conn" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/LinearLayout01"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:layout_marginBottom="40dp" >
            
            <LinearLayout
                android:id="@+id/LinearLayout09"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="20dp"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/imgSTC"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="@drawable/bgimagecapture"
                    android:src="@drawable/capture" />

                <TextView
                    android:id="@+id/TextView08"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="@string/photo_stc" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/LinearLayout08"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/imgSEC"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:background="@drawable/bgimagecapture"
                    android:src="@drawable/capture" />

                <TextView
                    android:id="@+id/TextView07"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="@string/photo_sec" />
            </LinearLayout>

        </LinearLayout>
                <LinearLayout
