                <TextView
                    android:id="@+id/txtWmn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="50"
                    android:gravity="right"
                    android:text="" />
            </LinearLayout>

            
            <LinearLayout
                android:id="@+id/lnHcn"
                android:visibility="gone"
